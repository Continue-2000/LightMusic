.mysongSheet{
    width: 100%;
    height: 100vh;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.songSheetItem{
    display: flex;
    align-items: center;
    width: 90%;
    height: 100rpx;
    margin-top: 10rpx;
}
.songSheetItem .image{
    width: 100rpx;
    height: 100rpx;
    background-color: purple;
    border-radius: 10rpx;
    overflow: hidden;
}
.songSheetItem .image image{
    width: 100%;
    height: 100%;
    background-size: 100% 100%;
}
.songSheetItem .info{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    height: 90rpx;
    margin-left: 20rpx;
}
.songSheetItem .info .title{
    font-size: 35rpx;
    color: #000;
    height:40rpx;
    max-width: 500rpx;
    white-space: nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
}
.songSheetItem .info .introduce{
    font-size: 30rpx;
    color: #ccc;
    max-width: 500rpx;
    height: 40rpx;
    white-space: nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
}