@import "/template/scrollTags/scrollTags.wxss";
.songSheetContainer{
  margin-top: 80rpx;
height: calc(100vh - 80rpx);
padding: 15rpx ;
}
.songSheetSwiper{
  width: 100%;
  height: 100%;
}
.content{
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  height: calc(100vh - 80rpx);
}
.contentItem{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 33.3%;
  height: 284rpx;
}

.contentItem image{
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
}
.itemIntroduce{
  width: 200rpx;
  height: 52rpx;
  margin-top: 10rpx;
  font-size: 26rpx; 
}